package com.sealblock.wallet;

/**
 * @author Luke
 */
public class SealblockException extends Exception {
	private static final long serialVersionUID = -2582406270405666287L;

	// 错误信息
	private String message;

	// 错误码
	private Integer code;

	public SealblockException(Integer code, String message) {
		this.code = code;
		this.message = message;
	}

	/**
	 * @return the message
	 */
	public String getMessage() {
		return message;
	}

	/**
	 * @return the code
	 */
	public Integer getCode() {
		return code;
	}

}
