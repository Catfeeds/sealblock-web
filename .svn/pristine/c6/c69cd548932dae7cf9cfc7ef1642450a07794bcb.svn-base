package com.woodare.template.jpa.model.data;
/**
 * 活动状态
 * @author windows
 *
 */
public enum EnumActivityState {
	//过程中			为开始			完成
	IN_PROCESS(1), NOT_START(2), FINISHED(3);

	private int value = 0;

	private EnumActivityState(int value) {
		this.value = value;
	}

	public String toString() {
		return String.valueOf(this.value);
	}
}
