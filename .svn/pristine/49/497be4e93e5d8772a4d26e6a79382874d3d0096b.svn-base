package com.thirdparty.passway._data;

import com.woodare.template.jpa.model.DownMerchantDeposit;
import com.woodare.template.jpa.model.DownNoCardInvoice;
import com.woodare.template.jpa.model.DownNoCardInvoiceTransfer;

/**
 * @author Luke
 */
public class PasswayRequestData extends BaseRequestData {
	private static final long serialVersionUID = -7251590985814056971L;

	// 交易表
	private DownNoCardInvoice invoice;

	// 代付表
	private DownMerchantDeposit deposit;

	// 代付附加表
	private DownNoCardInvoiceTransfer transfer;

	// 短信验证码
	private String verifyCode;

	// 异步回调地址
	private String notifyUrl;

	// Web同步回调地址
	private String frontUrl;

	private String extraText;

	private Object extraObj;

	/**
	 * @return the transfer
	 */
	public DownNoCardInvoiceTransfer getTransfer() {
		return transfer;
	}

	/**
	 * @param transfer
	 *            the transfer to set
	 */
	public void setTransfer(DownNoCardInvoiceTransfer transfer) {
		this.transfer = transfer;
	}

	/**
	 * @return the deposit
	 */
	public DownMerchantDeposit getDeposit() {
		return deposit;
	}

	/**
	 * @param deposit
	 *            the deposit to set
	 */
	public void setDeposit(DownMerchantDeposit deposit) {
		this.deposit = deposit;
	}

	/**
	 * @return the invoice
	 */
	public DownNoCardInvoice getInvoice() {
		return invoice;
	}

	/**
	 * @param invoice
	 *            the invoice to set
	 */
	public void setInvoice(DownNoCardInvoice invoice) {
		this.invoice = invoice;
	}

	/**
	 * @return the verifyCode
	 */
	public String getVerifyCode() {
		return verifyCode;
	}

	/**
	 * @param verifyCode
	 *            the verifyCode to set
	 */
	public void setVerifyCode(String verifyCode) {
		this.verifyCode = verifyCode;
	}

	/**
	 * @return the notifyUrl
	 */
	public String getNotifyUrl() {
		return notifyUrl;
	}

	/**
	 * @param notifyUrl
	 *            the notifyUrl to set
	 */
	public void setNotifyUrl(String notifyUrl) {
		this.notifyUrl = notifyUrl;
	}

	/**
	 * @return the frontUrl
	 */
	public String getFrontUrl() {
		return frontUrl;
	}

	/**
	 * @param frontUrl
	 *            the frontUrl to set
	 */
	public void setFrontUrl(String frontUrl) {
		this.frontUrl = frontUrl;
	}

	/**
	 * @return the extraText
	 */
	public String getExtraText() {
		return extraText;
	}

	/**
	 * @param extraText
	 *            the extraText to set
	 */
	public void setExtraText(String extraText) {
		this.extraText = extraText;
	}

	/**
	 * @return the extraObj
	 */
	public Object getExtraObj() {
		return extraObj;
	}

	/**
	 * @param extraObj
	 *            the extraObj to set
	 */
	public void setExtraObj(Object extraObj) {
		this.extraObj = extraObj;
	}
}
