package com.woodare.sealblock.util;

/**
 * @author Luke
 */
public class SealblockRequestException extends SealblockException {
	private static final long serialVersionUID = 4541477171273085595L;

	/**
	 * @param message
	 */
	public SealblockRequestException(String message) {
		super(message);
	}

	/**
	 * @param message
	 * @param throwable
	 */
	public SealblockRequestException(String message, Throwable throwable) {
		super(message, throwable);
	}
}
